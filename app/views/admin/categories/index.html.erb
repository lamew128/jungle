<section>

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

  <div class="well">
    <%= link_to '+ New Category', [:new, :admin, :category], class: 'btn btn-info' %>
  </div>

  <div>
    <% @category.each do |cat| %>
      <p><%= cat.id %>: <%=cat.name %></p>
    <% end %>
  </div>

</section>
